# type: =item
#. type: =item
#: dh_installlogcheck:35
msgid "debian/I<package>.logcheck.ignore.paranoid"
msgstr "debian/I<paquet>.logcheck.ignore.paranoid"

# type: textblock
#. type: textblock
#: dh_installlogcheck:37
